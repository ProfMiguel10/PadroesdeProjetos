package padroes.comportamentais.chain;
abstract class SuporteHandler {
    protected SuporteHandler proximo;
    public SuporteHandler setProximo(SuporteHandler p) {
        this.proximo = p;
        return p;
    }
    public abstract void tratar(String tipo);
}
class SuporteTI extends SuporteHandler {
    public void tratar(String tipo) {
        if (tipo.equals("senha")) {
            System.out.println("TI");
        } else if (proximo != null) proximo.tratar(tipo);
    }
}
class SecretariaAcademica extends SuporteHandler {
    public void tratar(String tipo) {
        if (tipo.equals("matricula")) {
            System.out.println("Secretaria");
        } else if (proximo != null) proximo.tratar(tipo);
    }
}
class Coordenacao extends SuporteHandler {
    public void tratar(String tipo) {
        if (tipo.equals("aproveitamento")) {
            System.out.println("Coordenação");
        } else System.out.println("Resolver");
    }
}
